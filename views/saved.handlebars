    {{#each saved}}
    <div class="card" id={{this._id}}>
        <div class="card-header" style="background-color: #2f7df6; color: white;">
            SubCategory
        </div>
        <div class="card-body">
            <div class="info">
                <h5 class="card-title">{{this.title}}</h5>
                <p class="card-text">{{this.summary}}</p>
                <span><a href="{{this.link}}">Full Article</a></span><br>
                <a href="/saved" class="btn btn-warning ">Article Notes</a>
                <a href="/saved" class="btn btn-danger">Delete from Saved</a>
            </div>
            <div class="image img-thumbnail">
                <img ssrc="{{this.img}}" width=200px height=200px>
            </div>  
        </div>
    </div>
    {{/saved}}